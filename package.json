{
  "name": "nextjs-cloudflare-workers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p 'dev:*'",
    "dev:next": "next dev --turbopack",
    "dev:path": "pathpida --ignorePath .gitignore --watch --enableStatic",
    "dev:docs": "redocly preview-docs",
    "build": "next build",
    "typecheck": "tsc -p . --noEmit",
    "start": "next start",
    "lint": "run-s 'lint:*'",
    "lint:biome-format": "biome format .",
    "lint:biome-lint": "biome check .",
    "lint:redocly": "redocly lint",
    "fix": "run-s 'fix:*'",
    "fix:biome-format": "biome format --write .",
    "fix:biome-lint": "biome check --write .",
    "migrate:generate": "drizzle-kit generate",
    "migrate:local": "wrangler d1 migrations apply nextjs-cloudflare-workers-db --local",
    "migrate:local:reset": "rm -rf .wrangler/state/v3/d1/miniflare-D1DatabaseObject",
    "migrate:prod": "wrangler d1 migrations apply nextjs-cloudflare-workers-db --remote",
    "schema:generate": "tsp compile ./schema --output-dir ./schema/out --emit=@typespec/openapi3 && orval --config orval.config.ts",
    "meta:generate": "pnpm tsx ./scripts/saveSites.ts",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "ts:local": "DATABASE_URL=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) tsx",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.0.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@typespec/best-practices": "0.46.0-dev.0",
    "@typespec/compiler": "^1.1.0",
    "@typespec/http": "^1.1.0",
    "@typespec/openapi": "^1.1.0",
    "@typespec/openapi3": "^1.1.0",
    "@typespec/rest": "^0.71.0",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "es-toolkit": "^1.39.3",
    "jose": "^6.0.11",
    "jotai": "^2.12.5",
    "lucide-react": "^0.515.0",
    "neverthrow": "^8.2.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "ulid": "^3.0.1",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@miniflare/d1": "^2.14.4",
    "@miniflare/shared": "^2.14.4",
    "@redocly/cli": "^1.34.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.17.47",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "3.2.3",
    "drizzle-kit": "^0.31.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.1",
    "orval": "^7.10.0",
    "pathpida": "^0.25.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5",
    "vitest": "^3.2.3",
    "wrangler": "^4.15.2"
  }
}
